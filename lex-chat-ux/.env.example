AWS_REGION=ap-northeast-2
LEX_BOT_ID=YOUR_BOT_ID
LEX_BOT_ALIAS_ID=YOUR_ALIAS_ID
LEX_LOCALE_ID=ko_KR

# 기본 엔진: aws-lex | ollama | openai-compatible
DEFAULT_AI_ENGINE=aws-lex
ENABLED_AI_ENGINES=aws-lex,ollama,openai-compatible

# For suggestions: if not provided, server will try Lex Models API (needs permissions).
BRANCH_VALUES=강남점,서초점,송파점
COURSE_VALUES=토익,토플,회화,자격증
# DRAFT is usually fine to read slot types; if you lock to numbered versions, set it.
LEX_MODEL_BOT_VERSION=DRAFT

# On-prem Ollama (Docker)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama3.1:8b

# On-prem OpenAI-compatible endpoint (vLLM/TGI 등)
OPENAI_COMPAT_BASE_URL=http://localhost:8000
OPENAI_COMPAT_MODEL=qwen2.5-7b-instruct
OPENAI_COMPAT_API_KEY=dummy

PORT=3000
